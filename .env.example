# Copy this to .env and fill in your actual values

# Telegram Bot Configuration
# Get this from @BotFather
TELEGRAM_BOT_TOKEN=

# Your Vercel webhook URL (leave empty for development with polling)
# Format: https://your-project.vercel.app/api/webhook
TELEGRAM_WEBHOOK_URL=

# Google Sheets Configuration
# The ID from your spreadsheet URL
GOOGLE_SHEETS_ID=

# Service account email from your JSON key file
GOOGLE_SERVICE_ACCOUNT_EMAIL=

# Private key from your JSON key file (keep the quotes and newlines)
GOOGLE_PRIVATE_KEY=

# Application Settings
LOW_STOCK_THRESHOLD=10
